{
  "name": "markmap-standalone",
  "version": "1.0.0",
  "description": "Standalone markmap executable",
  "main": "markmap-wrapper.js",
  "bin": {
    "markmap-standalone": "./markmap-wrapper.js"
  },
  "dependencies": {
    "markmap-cli": "^0.16.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "scripts": {
    "build": "pkg . --targets node18-win-x64 --output markmap-standalone.exe"
  },
  "pkg": {
    "assets": [
      "node_modules/markmap-cli/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  }
}
